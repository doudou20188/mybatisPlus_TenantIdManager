<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.book_crud.dao.BookDao">

    <select id="findBooksByName" resultType="com.example.book_crud.entity.vo.BookVO">
        select id,type,name,description from tbl_book where name = #{name}
    </select>


</mapper>
